<?xml version="1.0"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Decifr REST</title>
    <style>
        p, h2 {
            margin-left: 50px;
        }
    </style>
  </head>
  <body>
    <h2>Run ids</h2>
    {% for runid in runids %}
    <p><a href="/run/{{ runid }}">{{ runid }}</a></p>
    {% endfor %}

     <h2>Run ids with updated voucher</h2>
    {% for runid in runids_updated %}
    <p><a href="/run/{{ runid }}">{{ runid }}</a></p>
    {% endfor %}
</body>
</html>
